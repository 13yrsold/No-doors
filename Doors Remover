game.GetService("Players").LocalPlayer.Chatted:connect(function(msg)

if (string.lower(msg) == "nodoors", "nd") then
     local doors = get.Workspace:GetChildren()
     for i, v in pairs (doors)do
        if v.Name == "Door" then
            v:Destroy

        end
    end 
end
end)

--// Chat command ^
--// Hotkey command â¤µ

local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

mouse.KeyDown:connect(functon(key)

if key:lower() == "n" then
    local doors = get.Workspace:GetChildren()
    for i, v in pairs (doors) do
        if v.Name == "Door" then
            v:Destroy
        end
    end
end)
